class Solution :      def searchBigSortedArray ( self , reader , target ) :          begin , end = 0 , 1 while reader . get ( end - 1 ) < target :              begin = end end *= 2  while begin + 1 < end :              mid = ( begin + end ) / 2 if reader . get ( mid ) < target :                  begin = mid  else :                  end = mid   if reader . get ( begin ) == target :              return begin  if reader . get ( end ) == target :              return end  return - 1    