class Solution :      def partitionArray ( self , nums , k ) :          if not nums :              return 0  left , right = 0 , len ( nums ) - 1 while left <= right :              while left <= right and nums [ left ] < k :                  left += 1  while left <= right and nums [ right ] >= k :                  right -= 1  if left > right :                  break  nums [ left ] , nums [ right ] = nums [ right ] , nums [ left ]  return left    