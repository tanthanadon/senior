class Solution :      def canJump ( self , A ) :          if not A :              return False  len_A = len ( A ) if len_A == 1 or A [ 0 ] >= len_A - 1 :              return True  dp = [ False ] * len_A dp [ 0 ] = True for idx in xrange ( len_A ) :              if dp [ - 1 ] :                  return True  if not dp [ idx ] :                  continue  if A [ idx ] >= len_A - 1 - idx :                  return True  for jump_to in xrange ( 1 , A [ idx ] + 1 ) :                  dp [ idx + jump_to ] = True   return False    