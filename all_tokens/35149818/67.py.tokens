class Solution :      def binarySearch ( self , nums , target ) :          if not nums or not target : return - 1 begin , end = 0 , len ( nums ) - 1 while begin + 1 < end :              mid = ( begin + end ) / 2 mid_value = nums [ mid ] if mid_value >= target :                  end = mid  else :                  begin = mid   if nums [ begin ] == target :              return begin  if nums [ end ] == target :              return end  return - 1    