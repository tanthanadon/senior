class Solution ( object ) :      def averageOfLevels ( self , root ) :          q = collections . deque ( [ root ] ) result = [ ] while q :              cur_level_len = len ( q ) cur_sum = 0 for i in xrange ( cur_level_len ) :                  cur_node = q . pop ( ) cur_sum += cur_node . val if cur_node . right :                      q . appendleft ( cur_node . right )  if cur_node . left :                      q . appendleft ( cur_node . left )   result . append ( cur_sum * 1.0 / cur_level_len )  return result    