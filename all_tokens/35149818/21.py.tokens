class Solution ( object ) :      def topKFrequent ( self , nums , k ) :          len_nums = len ( nums ) bucket = [ None ] * ( len_nums + 1 ) d = { } for n in nums :              if n in d :                  d [ n ] += 1  else :                  d [ n ] = 1   for key in d . keys ( ) :              freq = d [ key ] if not bucket [ freq ] :                  bucket [ freq ] = [ key ]  else :                  bucket [ freq ] . append ( key )   result = [ ] for b in bucket [ : : - 1 ] :              if not b :                  continue  result += b if len ( result ) >= k :                  result = result [ : k ] break   return result    