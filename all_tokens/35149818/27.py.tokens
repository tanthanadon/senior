class Solution :      def longestCommonSubsequence ( self , A , B ) :          if not A or not B :              return 0  prev_row = [ 0 ] * ( len ( B ) + 1 ) for i in xrange ( 1 , len ( A ) + 1 ) :              cur_row = [ 0 ] for j in xrange ( 1 , len ( B ) + 1 ) :                  if A [ i - 1 ] == B [ j - 1 ] :                      cur_row . append ( prev_row [ j - 1 ] + 1 )  else :                      cur_row . append ( max ( prev_row [ j ] , cur_row [ j - 1 ] ) )   prev_row = cur_row  return cur_row [ - 1 ]    