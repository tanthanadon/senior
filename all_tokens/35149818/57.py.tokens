class Solution ( object ) :      def groupAnagrams ( self , strs ) :          if not strs :              return [ ]  d = { } for s in strs :              k = self . gen_key ( s ) if k in d :                  d [ k ] . append ( s )  else :                  d [ k ] = [ s ]   return d . values ( )  def gen_key ( self , s ) :          m = [ 0 ] * 26 for c in s :              m [ ord ( c ) - ord ( <str> ) ] += 1  result = <str> for idx , val in enumerate ( m ) :              if val > 0 :                  result += chr ( idx ) + str ( val )   return result    