class Solution :      def continuousSubarraySum ( self , A ) :          if not A :              return [ ]  cur_sum , max_sum = - 1 , float ( <str> ) start = end = - 1 result = [ start , end ] for i in xrange ( len ( A ) ) :              a = A [ i ] if cur_sum < 0 :                  cur_sum = a start = end = i  else :                  cur_sum += a end = i  if cur_sum > max_sum :                  result = [ start , end ] max_sum = cur_sum   return result    