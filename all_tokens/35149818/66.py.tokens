class Solution :      def twoSum6 ( self , nums , target ) :          if not nums or len ( nums ) < 2 :              return 0  nums . sort ( ) begin , end , result = 0 , len ( nums ) - 1 , 0 while begin < end :              cur_sum = nums [ begin ] + nums [ end ] if cur_sum == target :                      result += 1  if cur_sum >= target :                  while begin < end and nums [ end - 1 ] == nums [ end ] :                      end -= 1  end -= 1  else :                  while begin < end and nums [ begin + 1 ] == nums [ begin ] :                      begin += 1  begin += 1   return result    