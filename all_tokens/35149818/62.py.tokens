class Solution :      def findPeak ( self , A ) :          begin , end = 1 , len ( A ) - 2 while begin + 1 < end :              mid = ( begin + end ) / 2 if A [ mid ] < A [ mid + 1 ] :                  begin = mid  elif A [ mid - 1 ] > A [ mid ] :                  end = mid  else :                  return mid   if A [ begin ] > A [ end ] :              return begin  return end    