class Solution ( object ) :      def isSubsequence ( self , s , t ) :          if not s and not t :              return True  if not s :              return True  if not t :              return False  i_s = i_t = 0 while i_s < len ( s ) and i_t < len ( t ) :              if s [ i_s ] == t [ i_t ] :                  i_s += 1  i_t += 1  return i_s == len ( s )    