class Solution ( object ) :      def depthSum ( self , nestedList ) :          if not nestedList :              return 0  result = 0 level = 0 q = collections . deque ( nestedList ) while len ( q ) > 0 :              cur_len = len ( q ) level += 1 for i in xrange ( cur_len ) :                  cur_node = q . pop ( ) if cur_node . isInteger ( ) :                      result += cur_node . getInteger ( ) * level  else :                      for node in cur_node . getList ( ) :                          q . appendleft ( node )     return result    