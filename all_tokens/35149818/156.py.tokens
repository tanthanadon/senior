class Solution ( object ) :      def trap ( self , height ) :          if not height :              return 0  max_left = max_right = 0 left , right = 0 , len ( height ) - 1 max_water = 0 while left <= right :              if height [ left ] <= height [ right ] :                  if height [ left ] > max_left :                      max_left = height [ left ]  else :                      max_water += max_left - height [ left ]  left += 1  else :                  if height [ right ] > max_right :                      max_right = height [ right ]  else :                      max_water += max_right - height [ right ]  right -= 1   return max_water    