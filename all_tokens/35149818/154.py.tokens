class Solution :      def sortColors ( self , nums ) :          if not nums :              return  left , right , begin = 0 , len ( nums ) - 1 , 0 while begin <= right :              if nums [ begin ] == 0 :                  nums [ begin ] , nums [ left ] = nums [ left ] , nums [ begin ] left += 1 begin += 1  elif nums [ begin ] == 2 :                  nums [ begin ] , nums [ right ] = nums [ right ] , nums [ begin ] right -= 1  else :                  begin += 1      