class Solution :      def largestNumber ( self , nums ) :          if not nums :              return <str>  def my_cmp ( n1 , n2 ) :              case1 = str ( n1 ) + str ( n2 ) case2 = str ( n2 ) + str ( n1 ) if case1 > case2 :                  return - 1  elif case1 < case2 :                  return 1  return 0  nums . sort ( cmp = my_cmp ) result = <str> for n in nums :              result += str ( n )  return result if result [ 0 ] != <str> else <str>    