class Solution ( object ) :      def depthSumInverse ( self , nestedList ) :          if not nestedList :              return 0  weighted = unweighted = 0 while nestedList :              next_level = [ ] for nl in nestedList :                  if nl . isInteger ( ) :                      unweighted += nl . getInteger ( )  else :                      next_level += nl . getList ( )   weighted += unweighted nestedList = next_level  return weighted    